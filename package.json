{
  "name": "sourceshare",
  "version": "1.0.0",
  "description": "A simple SourceBin and srcShare API wrapper for Node.js",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": ["SourceBin", "srcShare", "API", "wrapper", "node", "node.js", "javascript", "js", "pasteBin", "hasteBin"],
  "author": "Squeaky2137",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint . --ext .ts --ignore-path .gitignore --fix",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.4.1",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "@sourcebin/linguist": "^0.0.3",
    "axios": "^1.3.4"
  },
  "files": ["lib/**/*"]
}
